			</exclusion>
			<exclusion>
				<artifactId>spring</artifactId>
				<groupId>org.springframework</groupId>
			</exclusion>
		</exclusions>
	</dependency>
	<dependency>
	 <groupId>org.javassist</groupId>
     <artifactId>javassist</artifactId>
